{    "name": "marco/telegram-bot",    "description": "Marcos Telegram Bot",    "prefer-stable": true,    "bin": [        "marcoBot.phar"    ],    "minimum-stability": "dev",    "require": {        "php": "*",        "ext-json": "*",        "ext-pdo": "*",        "guzzlehttp/guzzle": "^7.4"    },    "autoload": {        "psr-4": {            "Marco\\Telegram\\": "src/"        }    },    "scripts": {        "compile": "> marcoBot.phar && ./bin/box compile",        "fix": "./bin/phpcbf --standard=PSR12 src",        "check": "./bin/phpcs --standard=PSR12 -s  --colors --exclude=Generic.Files.LineLength src"    }}